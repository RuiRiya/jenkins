pipeline {
    agent any

stages {
        stage('print versions') {
            steps {
                sh 'npm cache verify'
                sh 'npm --version'
                sh 'node --version'
            }
        }

        stage('Install') {
            steps {
                sh 'npm install'
            }
        }

        stage('Build') {
            steps {
                sh 'npm run build'
            }
        }
    }
}
